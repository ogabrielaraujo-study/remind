{
    "expo": {
        "name": "Remind",
        "slug": "remind",
        "description": "Aplicativo de Tarefas",
        "version": "2.0.0",
        "privacy": "public",
        "sdkVersion": "32.0.0",
        "platforms": [
            "ios",
            "android"
        ],
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "primaryColor": "#fff",
        "androidStatusBarColor": "#085bc7",
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#085bc7"
        },
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "loading": {
            "hideExponentText": true,
            "backgroundImage": "./assets/splash.png"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "packagerOpts": {
            "assetExts": [
                "ttf"
            ]
        },
        "android": {
            "package": "com.project.remind"
        },
    }
}
